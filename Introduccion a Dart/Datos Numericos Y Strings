void main() {
  //Numeros
  int numero = 10; //int se usa para definir numeros
  double pi = 3.1415;//double para decimales
    
  print("Numero: $numero - $pi");
  
  //Strings - Cadenas de caracteres
  String nombre = "Ma'rio";
  print(nombre);
  //Obtener la primera posicion (letra) del nombre
  print(nombre[0]);
  //Obtener la ultima posicion (letra) del nombre
  print(nombre[nombre.length - 1]);
}
